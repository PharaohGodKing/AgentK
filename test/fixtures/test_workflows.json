{
  "research_workflow": {
    "id": "workflow_research_001",
    "name": "Comprehensive Research Workflow",
    "description": "Complete research workflow from topic analysis to report generation",
    "version": "1.0",
    "created_by": "system",
    "agents": ["research_agent", "analysis_agent", "archivist_agent"],
    "steps": [
      {
        "id": "topic_analysis",
        "agent": "research_agent",
        "action": "analyze_topic",
        "inputs": ["research_topic"],
        "outputs": ["research_plan"],
        "parameters": {
          "depth": "comprehensive",
          "sources": ["web", "local_db"]
        }
      },
      {
        "id": "information_gathering",
        "agent": "research_agent",
        "action": "gather_information",
        "inputs": ["research_plan"],
        "outputs": ["raw_data"],
        "parameters": {
          "max_sources": 10,
          "include_citations": true
        }
      },
      {
        "id": "data_analysis",
        "agent": "analysis_agent",
        "action": "analyze_data",
        "inputs": ["raw_data"],
        "outputs": ["insights"],
        "parameters": {
          "analysis_type": "comprehensive"
        }
      },
      {
        "id": "report_generation",
        "agent": "research_agent",
        "action": "generate_report",
        "inputs": ["insights"],
        "outputs": ["final_report"],
        "parameters": {
          "format": "markdown",
          "include_summary": true
        }
      },
      {
        "id": "archival",
        "agent": "archivist_agent",
        "action": "store_research",
        "inputs": ["final_report", "raw_data"],
        "outputs": ["storage_confirmation"],
        "parameters": {
          "categorize": true,
          "make_accessible": true
        }
      }
    ],
    "error_handling": {
      "retry_attempts": 3,
      "fallback_agent": "archivist_agent",
      "notify_on_failure": true
    },
    "metadata": {
      "creation_date": "2023-11-15",
      "last_modified": "2023-11-15",
      "tags": ["research", "analysis", "reporting"],
      "category": "research"
    }
  },
  "coding_workflow": {
    "id": "workflow_coding_001",
    "name": "Software Development Workflow",
    "description": "Complete coding workflow from requirements to deployment",
    "version": "1.0",
    "created_by": "system",
    "agents": ["coding_agent", "analysis_agent"],
    "steps": [
      {
        "id": "requirement_analysis",
        "agent": "analysis_agent",
        "action": "analyze_requirements",
        "inputs": ["project_description"],
        "outputs": ["specifications"],
        "parameters": {
          "detail_level": "comprehensive"
        }
      },
      {
        "id": "implementation",
        "agent": "coding_agent",
        "action": "write_code",
        "inputs": ["specifications"],
        "outputs": ["codebase"],
        "parameters": {
          "language": "auto_detect",
          "include_comments": true,
          "include_tests": true
        }
      },
      {
        "id": "testing",
        "agent": "coding_agent",
        "action": "test_code",
        "inputs": ["codebase"],
        "outputs": ["test_results"],
        "parameters": {
          "test_coverage": 0.8
        }
      },
      {
        "id": "documentation",
        "agent": "coding_agent",
        "action": "generate_docs",
        "inputs": ["codebase"],
        "outputs": ["documentation"],
        "parameters": {
          "format": "markdown"
        }
      }
    ],
    "error_handling": {
      "retry_attempts": 2,
      "fallback_agent": "analysis_agent",
      "notify_on_failure": true
    },
    "metadata": {
      "creation_date": "2023-11-15",
      "last_modified": "2023-11-15",
      "tags": ["coding", "development", "testing"],
      "category": "development"
    }
  }
}